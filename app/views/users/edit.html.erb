<h1>Editing</h1>
<%= form_for :user, url: user_edit_path(@user), method: :put do |f| %>
  <p>
    <%= f.label :firstname %><br>
    <%= f.text_field :firstname%>
  </p>
 
  <p>
    <%= f.label :secondname %><br>
    <%= f.text_field :secondname %>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email%>
  </p>
   
 
  <p>
    <%= f.submit %>
  </p>
<% end %>